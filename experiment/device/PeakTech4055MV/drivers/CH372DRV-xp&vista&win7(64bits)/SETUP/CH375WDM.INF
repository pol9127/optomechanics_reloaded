[Version]
Signature = "$Chicago$"
Class     = WCH
ClassGuid = {77989ADF-06DB-4025-92E8-40D902C03B0A}
Provider  = %WinChipHead%
DriverVer = 03/14/2011, 2.6.2011.03
CatalogFile.NT = CH375WDM.CAT

[ControlFlags]
