CH375.CopyFiles.DLL = 11
CH375.CopyFilesA64.DLL = 10, Syswow64 
CH375.CopyFilesI64.DLL = 10, Syswow64 
CH375.CopyFiles.SYS = 10, System32\Drivers
CH375.CopyFilesA64.SYS = 10, System32\Drivers
;CH375.CopyFilesI64.SYS = 10, System32\Drivers

[SourceDisksFiles]
