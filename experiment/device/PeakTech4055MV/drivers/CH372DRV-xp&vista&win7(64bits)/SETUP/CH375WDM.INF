[WinChipHead.NTamd64]
%CH375.DeviceDesc% = CH375.Install.NTamd64, USB\VID_4348&PID_5537
%CH375.DeviceDesc% = CH375.Install.NTamd64, USB\VID_1A86&PID_5537
